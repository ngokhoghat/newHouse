//- Master layout

doctype html
html(lang='en')
  head

    //- Meta
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
    meta(http-equiv='X-UA-Compatible', content='ie=edge')

    meta(name='msapplication-TileColor', content='#0E0E0E')
    meta(name='theme-color', content='#0E0E0E')

    //- SEO

    //- Basic Seo
    block basicSeo
      meta(name='description' content='Index page')
      meta(name='keywords', content='index, page')
      meta(name='author' content='')

    //- Twitter
    meta(name='twitter:card', content='summary')
    meta(name='twitter:site', content='')
    meta(name='twitter:image', content='')
    meta(name='twitter:description', content='')

    //- Facebook
    meta(property='og:url', content='')
    meta(property='og:title', content='')
    meta(property='og:description', content='')
    meta(property='og:image', content='')
    meta(property='og:type', content='website')


    //- Favicons
    link(rel='shortcut icon', href=require('images/favicons/favicon.ico'))

    //- Fonts
    //- link(href="https://fonts.googleapis.com/css?family=Quicksand", rel='stylesheet')

    //- Title
    block title
      title Your project!

  body

    //- block content
    block content

    include ../components/footer/footer.pug
    include ../components/globalComponent/navbarMobile/nav-mobile.pug
    include ../components/globalComponent/phoneContact/phoneContact.pug

    //- block scripts

    script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA_z_B6J5i0p5jw5FvXMoRtwZ7wdj2IwiE" defer)
    script(src="https://kit.fontawesome.com/a076d05399.js")
    
    block scripts